<h1>Houses#new</h1>
<p>Find me in app/views/houses/new.html.erb</p>
<%= form_for @new do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :house_word %>
  <%= f.text_field :house_word %>
  <%= f.label :img_url %>
  <%= f.text_field :img_url %>

  <%= f.submit "Create" %>
<% end %>
